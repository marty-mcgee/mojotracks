    "redux": "^3.7.2",
    "redux-thunk": "^2.0.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyify": "^5.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vinyl-source-stream": "^2.0.0",
    "stream-browserify": "^3.0.0",
