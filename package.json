    "webpack": "^5",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src/generic/",
      "<rootDir>/src/generic/scripts/",
      "<rootDir>/src/generic/scripts/app/",
      "<rootDir>/src/generic/styles/styles/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/src/generic/scripts/spec/setup.js",
      "jest-localstorage-mock"
    ]
  },
  "license": "ISC"
}
