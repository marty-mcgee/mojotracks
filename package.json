    "clone-deep": "^1.0.0",
    "copy-webpack-plugin": "^4.2.0",
    "deep-equal": "^1.0.1",
    "deep-extend": "^0.6.0",
    "html": "^1.0.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.1",
    "immutable": "^3.8.2",
    "immutable-ext": "^1.1.5",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "json-loader": "^0.5.7",
    "lzutf8": "^0.4.7",
    "raf": "^3.4.0",
    "ramda": "0.30.1",
    "ramda-fantasy": "0.8.0",
    "react-dragula": "^1.1.17",
    "react-fastclick": "^3.0.2",
    "reflect.ownkeys": "^0.2.0",
