{
  "name": "mojotracks",
  "version": "0.4.0-a0",
  "description": "Multi-Track Music Generator",
  "author": "Marty McGee @marty-mcgee | Ross McMillan @RossMcMillan92",
  "main": "",
  "scripts": {

    "build": "webpack --config=webpack.config.js --mode=development",
    "dev": "webpack serve --config=webpack.config.js --mode=development",
    "dev-serve-wp4": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js --config=webpack.config.js --progress --hot",
    "serve": "yarn dev",
    "start": "yarn dev",

    "build-origial": "SET NODE_ENV=production node node_modules/webpack/bin/webpack.js --config=webpack.config.js --env.platform=generic --colors",
    "dev-original": "SET NODE_ENV=development node node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --watch --hot --inline --config=webpack.config.js --env.platform=generic --env.isPhoneGap=false --colors",
    "prod-original": "SET NODE_ENV=production node node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --watch --config=webpack.config.js --env.platform=generic --env.isPhoneGap=false --colors",

    "spec": "jest --verbose --colors --verbose --coverage",
    "spec:watch": "jest --watch --colors",
    
    "phonegap": "SET NODE_ENV=production node node_modules/webpack/bin/webpack.js --config=webpack.config.js --env.platform=generic --env.isPhoneGap=true --colors",
    "phonegapdev": "SET NODE_ENV=development node node_modules/webpack/bin/webpack.js --config=webpack.config.js --env.platform=generic --env.isPhoneGap=true --colors --watch",
    
    "analyse": "node node_modules/webpack/bin/webpack.js --config=webpack.config.js --env.platform=generic --colors --json | node node_modules/webpack-bundle-size-analyzer/webpack-bundle-size-analyzer"

  },
  "dependencies": {
    "audiobuffer-to-wav": "^1.0.0",
